#include <stdio.h>
int main()
{
	int i = 0; j = 0; k = 0;
	// /bin/ls : /usr/bin/ls //
while (str[i] != NULL)
{
	char mo[150];
	char *string = "/bin/ls";
	char *str = "usr/bin/ls";
	if (str[i] == string[j] && str[i] == "/")
	{
	while (str[i] == string[j] && str[i] == "/" )
	{
		mo[k] == str[i];
		k++;
	}
	i++;
	j++;
	}
	printf("%s\n", mo);
}
